{
  "name": "circuitblocks",
  "description": "A graphical programming interface that helps newbies get into embedded programming.",
  "homepage": "https://circuitmess.com/",
  "author": {
    "email": "rx13@rwxrxrx.xyz",
    "name": "rwxr-xr-x"
  },
  "version": "1.10.1",
  "main": "build/app.js",
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.12",
    "@types/rimraf": "^3.0.2",
    "dom-parser": "^1.1.5",
    "electron-updater": "^6.1.8",
    "fs-extra": "^11.2.0",
    "google-protobuf": "^3.21.2",
    "js-yaml": "^4.1.0",
    "lzma-native": "^8.0.6",
    "node-localstorage": "^3.0.5",
    "platform-folders": "^0.6.0",
    "rimraf": "^5.0.5",
    "serialport": "^12.0.0",
    "sudo-prompt": "^9.2.1",
    "universal-analytics": "^0.5.3",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1"
  },
  "build": {
    "appId": "com.github.rx13.circuitblocks",
    "productName": "CircuitBlocks",
    "copyright": "Copyright Â© 2021 CircuitMess",
    "buildDependenciesFromSource": true,
    "npmRebuild": false,
    "afterSign": "scripts/notarize.cjs",
    "files": [
      "client/build/**/*",
      "build/**/*",
      "node_modules/**/*",
      "examples/**/*",
      "resources/**/*"
    ],
    "asarUnpack": [
      "**/*.node"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      "resources/ArduinoInstallerRO.dmg",
      "resources/Mac_OSX_VCP_Driver.zip",
      "resources/icon.*",
      "examples/**/*"
    ],
    "mac": {
      "helperBundleId": "com.github.rx13.circuitblocks",
      "category": "public.app-category.developer-tools",
      "icon": "resources/icon.icns",
      "hardenedRuntime": true,
      "entitlements": "resources/Entitlements.plist",
      "entitlementsInherit": "resources/Entitlements.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "sign": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "icon": "resources/icon.ico"
    }
  },
  "scripts": {
    "rebuild-electron-mac": "CC=/usr/local/Cellar/gcc@7/7.4.0_2/bin/gcc-7 CXX=/usr/local/Cellar/gcc@7/7.4.0_2/bin/g++-7 yarn electron-rebuild",
    "rebuild-electron": "CC=/usr/bin/gcc-7 CXX=/usr/bin/g++-7 yarn electron-rebuild",
    "build": "tsc",
    "clean": "rm -rf node_modules",
    "clean-install": "yarn clean && yarn install",
    "install-all": "cd client && NODE_ENV=development yarn && cd .. && NODE_ENV=development yarn && yarn electron-rebuild",
    "build-all": "cd client && yarn build && cd .. && yarn build && yarn dist",
    "electron-start": "yarn build && env ELECTRON_ENV=development electron build/app.js",
    "electron-watch": "nodemon -w app.js -w core/ --exec electron app.js",
    "dev": "concurrently --kill-others-on-fail \"cd client ; yarn dev\" \"wait-on http://localhost:3000 && yarn electron-start\"",
    "proto-js": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./build/core/grpc --grpc_out=./build/core/grpc --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I core/grpc/proto core/grpc/proto/cc/arduino/cli/commands/v1/*.proto",
    "proto-ts": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out core/grpc -I core/grpc/proto core/grpc/proto/cc/arduino/cli/commands/v1/*.proto",
    "proto": "yarn proto-js && yarn proto-ts",
    "pack": "electron-builder -wl --dir",
    "dist": "electron-builder -wl"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-flow": "^7.14.0",
    "@babel/plugin-transform-react-jsx": "^7.14.0",
    "@electron/notarize": "^2.3.0",
    "@electron/rebuild": "^3.6.0",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@types/electron": "^1.6.10",
    "@types/node": "^20.12.4",
    "@types/node-localstorage": "^1.3.3",
    "@types/universal-analytics": "^0.4.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "concurrently": "^8.2.2",
    "electron": "^29.2.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "prettier": "^3.2.5",
    "ts-proto": "^1.171.0",
    "typescript": "^5.4.4",
    "wait-on": "^7.2.0"
  }
}
