{
  "name": "circuitblocks-redux",
  "description": "CircuitBlocks Redux: a modernized, self-hosted graphical programming interface for embedded devices. Maintained fork of CircuitBlocks.",
  "homepage": "https://github.com/rx13/CircuitBlocks",
  "author": {
    "email": "rx13@rwxrxrx.xyz",
    "name": "rx13",
    "organization": "Holophage Limited"
  },
  "version": "1.10.1",
  "main": "build/app.js",
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "app-builder-lib": "^26.0.12",
    "dom-parser": "^1.1.5",
    "electron-updater": "6.6.2",
    "fs-extra": "^11.3.0",
    "google-protobuf": "^3.21.2",
    "js-yaml": "^4.1.0",
    "lzma-native": "^8.0.6",
    "node-fetch": "^3.3.2",
    "node-localstorage": "^3.0.5",
    "path-browserify": "^1.0.1",
    "platform-folders": "^0.6.0",
    "rimraf": "^6.0.1",
    "serialport": "13.0.0",
    "sudo-prompt": "^9.2.1",
    "universal-analytics": "^0.5.3",
    "unzipper": "0.12.3",
    "uuid": "11.1.0",
    "tar": "^7.4.3"
  },
  "build": {
    "appId": "com.github.rx13.circuitblocks",
    "productName": "CircuitBlocks Redux",
    "copyright": "Copyright Â© 2021-2025 Holophage Limited",
    "buildDependenciesFromSource": true,
    "npmRebuild": false,
    "afterSign": "scripts/notarize.cjs",
    "files": [
      "client/build/**/*",
      "build/**/*",
      "node_modules/**/*",
      "examples/**/*",
      "resources/**/*"
    ],
    "asarUnpack": [
      "**/*.node"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      "resources/ArduinoInstallerRO.dmg",
      "resources/Mac_OSX_VCP_Driver.zip",
      "resources/icon.*",
      "examples/**/*"
    ],
    "mac": {
      "helperBundleId": "com.github.rx13.circuitblocks",
      "category": "public.app-category.developer-tools",
      "icon": "resources/icon.icns",
      "hardenedRuntime": true,
      "entitlements": "resources/Entitlements.plist",
      "entitlementsInherit": "resources/Entitlements.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "sign": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "icon": "resources/icon.ico"
    }
  },
  "scripts": {
    "postinstall": "node scripts/rebuild-native.js",
    "rebuild-native": "node scripts/rebuild-native.js",
    "rebuild-native-force": "npm run clean && npm install && node scripts/rebuild-native.js",
    "rebuild-electron-mac": "CC=/usr/local/Cellar/gcc@7/7.4.0_2/bin/gcc-7 CXX=/usr/local/Cellar/gcc@7/7.4.0_2/bin/g++-7 yarn @electron/rebuild",
    "rebuild-electron": "CC=/usr/bin/gcc-7 CXX=/usr/bin/g++-7 yarn @electron/rebuild",
    "build": "tsc",
    "clean": "rm -rf node_modules",
    "clean-install": "yarn clean && yarn install",
    "install-all": "cd client && NODE_ENV=development yarn build && cd .. && NODE_ENV=development yarn build && NODE_ENV=production yarn electron-rebuild",
    "build-all": "cd client && NODE_ENV=development yarn build && cd .. && NODE_ENV=development yarn build && NODE_ENV=production yarn dist --${npm_config_platform:-$(node -p \"process.platform\")}",
    "electron-start": "yarn build && env ELECTRON_ENV=development electron build/app.js",
    "electron-watch": "nodemon -w app.js -w core/ --exec electron app.js",
    "dev": "NODE_ENV=development concurrently --kill-others-on-fail \"cd client ; npm run dev\" \"wait-on http://localhost:8080 && yarn electron-start\"",
    "proto-js": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./build/core/grpc --grpc_out=./build/core/grpc --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I core/grpc/proto core/grpc/proto/cc/arduino/cli/commands/v1/*.proto",
    "proto-ts": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out core/grpc -I core/grpc/proto core/grpc/proto/cc/arduino/cli/commands/v1/*.proto",
    "proto": "yarn proto-js && yarn proto-ts",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --${npm_config_platform:-$(node -p \"process.platform\")}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.14.0",
    "@babel/plugin-transform-react-jsx": "^7.14.0",
    "@electron/notarize": "3.0.1",
    "@electron/rebuild": "4.0.1",
    "@mapbox/node-pre-gyp": "^2.0.0",
    "@types/electron": "^1.6.12",
    "@types/node": "^24.1.0",
    "@types/node-localstorage": "^1.3.3",
    "@types/rimraf": "^4.0.5",
    "@types/universal-analytics": "^0.4.8",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "9.2.0",
    "dmg-builder": "26.0.12",
    "electron": "37.2.5",
    "electron-builder": "26.0.12",
    "electron-builder-squirrel-windows": "26.0.12",
    "eslint": "^9.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "path-scurry": "^2.0.0",
    "prettier": "^3.6.2",
    "ts-proto": "2.7.5",
    "typescript": "^5.8.3",
    "wait-on": "8.0.4"
  }
}
